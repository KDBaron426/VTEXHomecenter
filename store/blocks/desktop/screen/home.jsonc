{
  "responsive-layout.desktop": {
    "children": [
      "whatsapp-button",
      "flex-layout.row#desktop--location",
      "slider-layout#home__slider-desktop--children",
      "flex-layout.row#home__section--one",
      "flex-layout.row#home__section--two",
      "flex-layout.row#home__section--three",
      "flex-layout.row#home__section--four",
      "flex-layout.row#home__section--five",
      "flex-layout.row#home__shelf--one",
